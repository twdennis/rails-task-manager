<h1> <%= @task.title  %></h1>
 <p> <%= @task.details %></p>
 <p>
  <% if @task.completed %>
  Task completed
  <% else %>
  Task not complete
  <% end %>
</p>
<%= link_to "Edit", edit_path %><br>
<%= link_to "Delete",
  task_path(@task),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%> 
<%= link_to "Back to list", tasks_path %>
